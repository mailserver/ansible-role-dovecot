auth_mechanisms = plain login
disable_plaintext_auth = yes

auth_debug=no

passdb {
  driver = sql 
  args = {{ _dovecot_config.config_dir }}/auth-sql.conf.ext 
}
userdb {
  driver = sql
  default_fields = uid={{ vmail_user }} gid={{ vmail_group }}
  args = {{ _dovecot_config.config_dir }}/auth-sql.conf.ext 
}

